{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "prettier",
    "react-app",
    "react-app/jest",
    "plugin:import/typescript",
    "plugin:prettier/recommended"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": ["error",{
      "endOfLine": "auto"}
    ]
  }
}
